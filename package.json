{
  "name": "academic-repository-system",
  "version": "1.0.0",
  "description": "Academic Repository System - Full Stack Application",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && uvicorn main:app --reload",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "docker-compose up -d"
  },
  "keywords": [
    "academic",
    "repository",
    "fastapi",
    "react",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
